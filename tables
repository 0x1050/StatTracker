//This document walks you through the creation of the database and tables.
//I have also listed some helpful MySQL commands that you may find useful.
//You can just copy/paste these lines into your MySQL command line to
//create and populate some test tables.

CREATE DATABASE IF NOT EXISTS StatTracker;

USE StatTracker ;

//Put your username before the @ symbol
GRANT ALL PRIVILEGES ON StatTracker.* TO 'username'@'localhost'

CREATE TABLE Users( UserID       INT(2)      UNSIGNED NOT NULL AUTO_INCREMENT PRIMARY KEY,
                    username     VARCHAR(20) NOT NULL,
                    email        CHAR(60)    NOT NULL,
                    password     CHAR(60)    NOT NULL,
                    groupNumber  INT(1)      NOT NULL,
                    theme        INT(1)      NOT NULL DEFAULT 1,
                    active       INT(1)      NOT NULL DEFAULT 0,
                    stayLoggedIn INT(1)      NOT NULL DEFAULT 0,
                    s1           INT(1)      NOT NULL DEFAULT 0,
                    s2           INT(1)      NOT NULL DEFAULT 0,
                    s3           INT(1)      NOT NULL DEFAULT 0,
                    s4           INT(1)      NOT NULL DEFAULT 0,
                    s5           INT(1)      NOT NULL DEFAULT 0,
                    s6           INT(1)      NOT NULL DEFAULT 0
                    ) ;

