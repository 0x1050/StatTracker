CREATE TABLE G1(LikeA    INT(1) DEFAULT NULL,
                LikeB    INT(1) DEFAULT NULL,
                LIKEC    INT(1) DEFAULT NULL,
                DislikeA INT(1) DEFAULT NULL,
                DislikeB INT(1) DEFAULT NULL,
                DislikeC INT(1) DEFAULT NULL,
                A1       INT(1) DEFAULT NULL,
                A2       INT(1) DEFAULT NULL,
                A3       INT(1) DEFAULT NULL,
                B1       INT(1) DEFAULT NULL,
                B2       INT(1) DEFAULT NULL,
                B3       INT(1) DEFAULT NULL,
                C1       INT(1) DEFAULT NULL,
                C2       INT(1) DEFAULT NULL,
                C3       INT(1) DEFAULT NULL,
                Scale    INT(1) NOT NULL,
                FF1      TINYTEXT,
                FF2      TINYTEXT);

